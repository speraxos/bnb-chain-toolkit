{
  "name": "@nirholas/crypto-market-data",
  "version": "1.0.0",
  "description": "Comprehensive cryptocurrency market data service with CoinGecko, DeFiLlama, and Fear & Greed Index integration. Built-in caching, rate limiting, and Edge Runtime compatibility.",
  "keywords": [
    "crypto",
    "cryptocurrency",
    "market-data",
    "coingecko",
    "defillama",
    "bitcoin",
    "ethereum",
    "defi",
    "tvl",
    "fear-greed-index",
    "price-api",
    "trading",
    "edge-runtime"
  ],
  "author": "nirholas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirholas/crypto-market-data.git"
  },
  "homepage": "https://github.com/nirholas/crypto-market-data#readme",
  "bugs": {
    "url": "https://github.com/nirholas/crypto-market-data/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
